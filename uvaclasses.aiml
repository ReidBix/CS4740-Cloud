<category>
	<pattern>WHEN DOES * MEET</pattern>
	<template><star/> meets at <system>python when_class.py <star/></system></template>
</category>
<category>
	<pattern>WHERE DOES * MEET</pattern>
	<template><star/> meets at <system>python where_class.py <star/></system></template>
</category>
